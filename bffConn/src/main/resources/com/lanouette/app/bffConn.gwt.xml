<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='bffConn'>
    <!-- Inherit the core Web Toolkit stuff.                        -->
    <inherits name='com.google.gwt.user.User'/>
    <!--inherits name='com.google.gwt.inject.Inject'/-->

    <!-- Inherit the default GWT style sheet.  You can change       -->
    <!-- the theme of your GWT application by uncommenting          -->
    <!-- any one of the following lines.                            -->
    <inherits name='com.google.gwt.user.theme.standard.Standard'/>

    <!--<inherits name="com.google.gwt.logging.Logging"/>-->
    <inherits name='com.google.gwt.visualization.Visualization'/>
    <!--<inherits name='com.smartgwt.SmartGwt'/>-->
    <!--<inherits name='com.smartclient.theme.graphite.Graphite'/>-->
    <inherits name="com.smartgwt.SmartGwtNoScript"/>

    <inherits name="org.fusesource.restygwt.RestyGWT"/>
    <!--<inherits name="com.googlecode.mgwt.MGWT"/>-->
    <!--<set-property name="user.agent" value="safari" />-->
    <!-- <inherits name='com.google.gwt.user.theme.chrome.Chrome'/> -->
    <!-- <inherits name='com.google.gwt.user.theme.dark.Dark'/>     -->

    <!--<set-configuration-property name='xsiframe.failIfScriptTag' value='FALSE'/>-->

    <!-- Other module inherits                                      -->
    <inherits name='com.allen_sauer.gwt.log.gwt-log-DEBUG'/>
    <inherits name="com.google.gwt.json.JSON"/>
    <inherits name="com.googlecode.mgwt.dom.DOM"/>
    <inherits name="com.googlecode.mgwt.ui.UI"/>
    <set-property name="log_DivLogger" value="DISABLED"/>

    <!-- Specify the app entry point class.                         -->
    <entry-point class='com.lanouette.app.client.bffConn'/>

        <!-- Specify the paths for translatable code                    -->
    <source path='client'/>
    <source path='shared'/>

    <add-linker name="xsiframe"/>
    <set-configuration-property name="devModeRedirectEnabled" value="true"/>
    <set-property name="compiler.useSourceMaps" value="true" />
</module>
